    "react-tag-autocomplete": "^6.3.0",
    "reactstrap": "^9.1.5",
    "redux": "^4.2.0",
    "redux-localstorage-simple": "^2.5.1",
    "redux-thunk": "^2.4.2",
    "uuid": "^8.3.2",
    "workbox-core": "^6.5.4",
