    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.10",
    "@types/react-tag-autocomplete": "^6.3.0",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^3.1.0",
    "adm-zip": "^0.5.10",
    "babel-jest": "^29.3.1",
    "chalk": "^5.2.0",
    "eslint": "^8.30.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.3.1",
